{
  "version": 2,
  "env": {
    "PORT": "@port",
    "MONGODB_URI": "@mongodb_uri",
    "MONGODB_NAME": "@@mongodb_name",
    "JWT_SECRET": "@jwt_secret",
    "JWT_EXPIRES": "@jwt_expires"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "OPTIONS"]
    }
  ]
}
